# platform :ios, '9.0'

def shared_pods
	pod 'Google-Mobile-Ads-SDK', '~> 7.21.0’
	pod 'Fabric'
	pod 'Crashlytics'
	pod 'Firebase/Core'
end

use_frameworks!

target 'Its Almost My Birthday' do
	shared_pods
end

target 'Its Almost Christmas' do
    shared_pods
end

target 'Its Almost Halloween' do
    shared_pods
end

target 'Es casi mi cumpleaños' do
	shared_pods
end
